<core:FragmentDefinition
        xmlns:core="sap.ui.core"
        xmlns="sap.m">
        <Dialog
                title="{i18n>errorTitle}"
                type="Message">
                <content>
                        <Text text="{errModel>/errMessage}" />
                </content>
                <beginButton>
                        <Button text="{i18n>nameRequiredDialogButton}" press="onDialogClose" />
                </beginButton>
        </Dialog>
</core:FragmentDefinition>